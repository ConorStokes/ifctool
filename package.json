{
  "name": "@bldrs-ai/ifctool",
  "version": "2.0.0",
  "main": "src/ifctool.js",
  "bin": "src/main.js",
  "homepage": "https://github.com/bldrs-ai/ifctool",
  "bugs": {
    "url": "https://github.com/bldrs-ai/ifctool/issues"
  },
  "keywords": [
    "ifc",
    "ifcJSON",
    "csv",
    "cli"
  ],
  "scripts": {
    "build": "node config/esbuild.js",
    "lint": "yarn eslint src",
    "ifctool": "node src/ifctool.js",
    "ifctool-dist": "node dist/ifctool.js",
    "test": "node --experimental-vm-modules $(yarn bin jest)",
    "precommit": "yarn lint && yarn test",
    "prepare": "husky install"
  },
  "dependencies": {
    "json2csv": "^5.0.7",
    "web-ifc": "^0.0.35"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-import-assertions": "^7.16.7",
    "esbuild": "^0.14.22",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.0",
    "jest": "^27.4.3",
    "shx": "^0.3.4"
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [],
    "transform": {},
    "transformIgnorePatterns": [
      "/node_modules/(?!(web-ifc)/)"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": []
  },
  "description": "Command line tool for working with IFC models.  It extracts IFC elements by ID or types, and exports as JSON or CSV (via [json2csv](https://www.npmjs.com/package/json2csv)).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bldrs-ai/ifctool.git"
  },
  "author": "info@bldrs.ai",
  "license": "MIT"
}
